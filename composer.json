{
    "name": "aboutcoders/supervisor-bundle",
    "type": "symfony-bundle",
    "description": "A symfony bundle to manage supervisor.",
    "keywords": ["supervisor", "supervisord", "process", "control", "system", "rest", "console", "command"],
    "homepage": "http://aboutcoders.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannes Schulz",
            "email": "hannes.schulz@aboutcoders.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/symfony": "~2.6|~3.0",
        "supervisorphp/supervisor": "~3",
        "lstrojny/fxmlrpc": "^0.13.0",
        "zendframework/zend-diactoros": "^1.3",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.3"
    },
    "require-dev": {
        "symfony/monolog-bundle": "~2"
    },
    "autoload": {
        "psr-4": { "Abc\\Bundle\\SupervisorBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
